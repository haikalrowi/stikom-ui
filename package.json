{
  "name": "stikom-ui",
  "version": "1.0.0",
  "description": "STIKOM UI Monorepo",
  "license": "MIT",
  "author": "STIKOM PGRI Banyuwangi",
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "format:check": "prettier . --check --plugin=prettier-plugin-tailwindcss",
    "format:write": "prettier . --write --plugin=prettier-plugin-tailwindcss",
    "sort-imports:check": "prettier . --check --plugin=@trivago/prettier-plugin-sort-imports --import-order-sort-specifiers --import-order-group-namespace-specifiers",
    "sort-imports:write": "prettier . --write --plugin=@trivago/prettier-plugin-sort-imports --import-order-sort-specifiers --import-order-group-namespace-specifiers"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "rollup": "^4.12.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  }
}
